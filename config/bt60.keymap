#include <behaviors.dtsi>
#include <dt-bindings/zmk/backlight.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/rgb.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            // ------------------------------------------------------------------------------------------
            // | ESC |  1  |  2  |  3  |  4  |  5  |  6  |  7  |  8  |  9  |  0  |  -  |  =  |BKSP | DEL |
            // | TAB  |  Q  |  W  |  E  |  R  |  T  |  Y  |  U  |  I  |  O  |  P  |  [  |  ]  |  |   |
            // | CAPS  |  A  |  S  |  D  |  F  |  G  |  H  |  J  |  K  |  L  |  ;  |  '  |     ENTER    |
            // |  SHIFT|NONE|  Z  |  X  |  C  |  V  |  B  |  N  |  M  |  ,  |  .  | /   | SHFT | UP | 1  |
            // |  CTL  |  WIN  |  ALT  |            SPACE              | ALT | CTRL | LEFT | DOWN | RIGHT |
            // ------------------------------------------------------------------------------------------

            bindings = <
&kp C_PLAY_PAUSE     &kp N1    &kp N2    &kp N3     &kp N4    &kp N5    &kp N6     &kp N7                         &kp N8     &kp N9    &kp N0  &kp MINUS           &kp EQUAL  &kp BACKSPACE  &kp K_MUTE
         &kp TAB      &kp Q     &kp W     &kp E      &kp R     &kp T     &kp Y      &kp U                          &kp I      &kp O     &kp P   &kp LBKT            &kp RBKT  &kp BACKSLASH
  &lt 1 CAPSLOCK      &kp A     &kp S     &kp D      &kp F     &kp G     &kp H      &kp J                          &kp K      &kp L  &kp SEMI    &kp SQT             &kp RET
       &kp LSHFT      &none     &kp Z     &kp X      &kp C     &kp V     &kp B      &kp N                          &kp M  &kp COMMA   &kp DOT   &kp FSLH  &mt RSHFT UP_ARROW          &none       &mo 1
                  &kp LCTRL  &kp LGUI  &kp LALT  &kp SPACE  &kp RALT  &kp LEFT  &kp RIGHT  &mt RIGHT_CONTROL RIGHT_ARROW      &none
            >;

            sensor-bindings = <&inc_dec_kp DOWN_ARROW UP_ARROW &inc_dec_kp C_VOLUME_DOWN C_VOLUME_UP>;
        };

        raise {
            bindings = <
      &kp GRAVE    &kp F1           &kp F2           &kp F3           &kp F4           &kp F5           &kp F6           &kp F7           &kp F8           &kp F9    &kp F10    &kp F11          &kp F12      &trans  &kp C_MUTE
&rgb_ug RGB_TOG    &trans           &kp UP       &bl BL_DEC       &bl BL_INC           &trans           &trans           &trans          &kp INS           &trans  &kp PSCRN   &kp SLCK  &kp PAUSE_BREAK  &bl BL_TOG
         &trans  &kp LEFT         &kp DOWN        &kp RIGHT           &trans           &trans           &trans           &trans           &trans           &trans   &kp HOME  &kp PG_UP      &bootloader
     &kp C_PREV     &none  &rgb_ug RGB_EFR  &rgb_ug RGB_EFF  &rgb_ug RGB_BRD  &rgb_ug RGB_BRI  &rgb_ug RGB_SAD  &rgb_ug RGB_SAI  &rgb_ug RGB_SPD  &rgb_ug RGB_SPI     &trans     &trans          &kp END   &kp PG_DN  &kp C_NEXT
                   &trans       &bt BT_PRV       &bt BT_NXT           &trans           &trans           &trans           &trans           &trans           &trans
            >;

            sensor-bindings = <&inc_dec_kp DOWN_ARROW UP_ARROW &inc_dec_kp C_VOLUME_DOWN C_VOLUME_UP>;
        };
    };
};
