#include <behaviors.dtsi>
#include <dt-bindings/zmk/backlight.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/rgb.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            // ------------------------------------------------------------------------------------------
            // | ESC |  1  |  2  |  3  |  4  |  5  |  6  |  7  |  8  |  9  |  0  |  -  |  =  | | | ENC1 |
            // | TAB  |  Q  |  W  |  E  |  R  |  T  |  Y  |  U  |  I  |  O  |  P  |  [  |  ]  |   BKSP |
            // | CAPS  |  A  |  S  |  D  |  F  |  G  |  H  |  J  |  K  |  L  |  ;  |  '  |    ENTER    |
            // |  SHIFT |  Z  |  X  |  C  |  V  |  B  |  N  |  M  |  ,  |  .  | /   |   SHFT   | DEL |
            // |  CTL  |  WIN  |  ALT  |            SPACE              | ALT  | LEFT  |  DOWN |  RIGHT |
            // ------------------------------------------------------------------------------------------

            bindings = <
&kp C_PLAY_PAUSE            &kp N1    &kp N2    &kp N3     &kp N4    &kp N5      &kp N6             &kp N7     &kp N8   &kp N9    &kp N0        &kp MINUS   &kp EQUAL  &kp BACKSPACE  &mo 1
         &kp TAB             &kp Q     &kp W     &kp E      &kp R     &kp T       &kp Y              &kp U      &kp I    &kp O     &kp P         &kp LBKT    &kp RBKT  &kp BACKSLASH
  &lt 1 CAPSLOCK             &kp A     &kp S     &kp D      &kp F     &kp G       &kp H              &kp J      &kp K    &kp L  &kp SEMI          &kp SQT     &kp RET
       &kp LSHFT             &kp Z     &kp X     &kp C      &kp V     &kp B       &kp N              &kp M  &kp COMMA  &kp DOT  &kp FSLH  &kp RIGHT_SHIFT  &kp DELETE
                  &kp LEFT_CONTROL  &kp LGUI  &kp LALT  &kp SPACE  &kp RALT  &kp K_MUTE  &kp RIGHT_CONTROL      &mo 1
            >;

            sensor-bindings = <&inc_dec_kp DOWN_ARROW UP_ARROW &inc_dec_kp C_VOLUME_DOWN C_VOLUME_UP>;
        };

        raise {
            bindings = <
     &kp K_MUTE        &kp F1        &kp F2        &kp F3        &kp F4  &kp F5  &kp F6  &kp F7   &kp F8  &kp F9    &kp F10   &kp F11          &kp F12  &kp ESCAPE  &kp C_MUTE
&rgb_ug RGB_TOG        &trans        &kp UP        &trans        &trans  &trans  &trans  &trans  &kp INS  &trans  &kp PSCRN  &kp SLCK  &kp PAUSE_BREAK  &kp DELETE
   &bl BL_CYCLE      &kp LEFT      &kp DOWN     &kp RIGHT        &trans  &trans  &trans  &trans   &trans  &trans   &kp HOME  &kp BSLH      &bootloader
     &bl BL_TOG  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4  &trans  &trans  &trans   &trans  &trans     &trans    &trans           &trans
                   &bt BT_CLR    &bt BT_PRV    &bt BT_NXT        &trans  &trans  &trans  &trans   &trans
            >;

            sensor-bindings = <&inc_dec_kp C_AC_SCROLL_DOWN C_AC_SCROLL_UP &inc_dec_kp PAGE_UP PAGE_DOWN>;
        };

        light {
            bindings = <
     &bt BT_CLR     &bt BT_SEL 0     &bt BT_SEL 1     &bt BT_SEL 2     &bt BT_SEL 3     &bt BT_SEL 4  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
&rgb_ug RGB_TOG  &rgb_ug RGB_BRI  &rgb_ug RGB_HUI  &rgb_ug RGB_EFF  &rgb_ug RGB_SPI  &rgb_ug RGB_SAI  &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
         &trans  &rgb_ug RGB_BRD  &rgb_ug RGB_HUD  &rgb_ug RGB_EFR  &rgb_ug RGB_SPD  &rgb_ug RGB_SAD  &trans  &trans  &trans  &trans  &trans  &trans  &trans
     &bl BL_TOG       &bl BL_DEC       &bl BL_INC     &bl BL_CYCLE           &trans           &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
                          &trans           &trans           &trans           &trans           &trans  &trans  &trans  &trans
            >;

            sensor-bindings = <&inc_dec_kp PAGE_UP PAGE_DOWN &inc_dec_kp PAGE_UP PAGE_DOWN>;
        };
    };
};
